 "font_size": 14,
 "html": "<style>\r\n    body {\r\n        font-family: Arial, sans-serif;\r\n        font-size: 12px;\r\n        color: #000;\r\n        padding: 5px;\r\n    }\r\n    .header-section {\r\n        width: 100%;\r\n        margin-bottom: 15px;\r\n    }\r\n    .header-section td {\r\n        padding: 3px 0;\r\n    }\r\n    .title {\r\n        text-align: center;\r\n        font-size: 16px;\r\n        font-weight: bold;\r\n        margin-bottom: 15px;\r\n    }\r\n    table.print-table {\r\n        width: 100%;\r\n        border-collapse: collapse;\r\n        font-size: 11px;\r\n    }\r\n    table.print-table th,\r\n    table.print-table td {\r\n        border: 1px solid #000;\r\n        padding: 4px;\r\n        text-align: left;\r\n    }\r\n    table.print-table th {\r\n        text-align: center;\r\n        font-weight: bold;\r\n    }\r\n    .no-border td {\r\n        border: none;\r\n        padding: 2px 0;\r\n    }\r\n    .footer {\r\n        width: 100%;\r\n        margin-top: 30px;\r\n        font-size: 12px;\r\n    }\r\n</style>\r\n\r\n<div class=\"title\">STOCK MOVEMENTS DETAILS</div>\r\n\r\n<table class=\"header-section\">\r\n    <tr>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Customer</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;width:40%;\">{{ doc.customer }}</td>\r\n        <td style=\"width:5%; padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Job Number</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;width:20%;\">{{ doc.name }}</td>\r\n    </tr>\r\n    <tr>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Attention</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;\">{{ doc.attention }}</td>\r\n        <td style=\"padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Job Date</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;\">{{ doc.date }}</td>\r\n    </tr>\r\n    <tr>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Period</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;\">{{ doc.date }}</td>\r\n        <td style=\"padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Warehouse</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;\">{{ doc.table_jjim[0].warehouse if doc.table_jjim else '' }}</td>\r\n    </tr>\r\n    <tr>\r\n        <td style=\"padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"></td>\r\n        <td style=\"padding: 3px 0 !important;\"><b>Branch</b></td>\r\n        <td style=\"padding: 3px 0 !important;\">:</td>\r\n        <td style=\"padding: 3px 0 !important;\">{{ doc.branch }}</td>\r\n    </tr>\r\n</table>\r\n\r\n<table class=\"print-table\">\r\n    <tr>\r\n        <th style=\"padding: 4px !important;\">No</th>\r\n        <th style=\"padding: 4px !important;\">Date</th>\r\n        <th style=\"padding: 4px !important;\">Warehouse</th>\r\n        <th style=\"padding: 4px !important;\">Descriptions</th>\r\n        <th style=\"padding: 4px !important;\">UoM</th>\r\n        <th style=\"padding: 4px !important;\">IN</th>\r\n        <th style=\"padding: 4px !important;\">OUT</th>\r\n        <th style=\"padding: 4px !important;\">GRN/GDN</th>\r\n        <th style=\"padding: 4px !important;\">In<br>Volume</th>\r\n        <th style=\"padding: 4px !important;\">Out<br>Volume</th>\r\n        <th style=\"padding: 4px !important;\">Balance</th>\r\n        <th style=\"padding: 4px !important;\">UoM</th>\r\n        <th style=\"padding: 4px !important;\">Remarks</th>\r\n    </tr>\r\n    \r\n    {% set ns = namespace(total_in=0.0, total_out=0.0, final_balance=0.0) %}\r\n    \r\n    {% for row in doc.table_jjim %}\r\n    \r\n        {% set ns.total_in = ns.total_in + (row.in_volume | float(0)) %}\r\n        {% set ns.total_out = ns.total_out + (row.out_volume | float(0)) %}\r\n        {% set ns.final_balance = (row.balance | float(0)) %}\r\n    \r\n        <tr>\r\n            <td style=\"padding: 4px !important;\">{{ loop.index }}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.date or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.warehouse or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.description or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.uom or '' }}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.in_qty or '' }}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.out_qty or '' }}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.grn or row.gdn or '' }}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.in_volume or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.out_volume or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.balance or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.volume_uom or ''}}</td>\r\n            <td style=\"padding: 4px !important;\">{{ row.remarks or ''}}</td>\r\n        </tr>\r\n    \r\n    {% endfor %}\r\n    \r\n    <tr>\r\n        <th colspan=\"8\" style=\"text-align:right; padding: 4px !important;\">Total</th>\r\n        <th style=\"padding: 4px !important;\">{{ \"%.2f\" | format(ns.total_in) }}</th>\r\n        <th style=\"padding: 4px !important;\">{{ \"%.2f\" | format(ns.total_out) }}</th>\r\n        <th style=\"padding: 4px !important;\">{{ \"%.2f\" | format(ns.final_balance) }}</th>\r\n        <th colspan=\"2\" style=\"padding: 4px !important;\"></th>\r\n    </tr>\r\n</table>\r\n\r\n<br>\r\n\r\n<table class=\"no-border\">\r\n    <tr>\r\n        <td style=\"padding: 2px 0 !important;\"><b>Balance Volume</b></td>\r\n        <td style=\"padding: 2px 0 !important;\">:</td>\r\n        <td style=\"padding: 2px 0 !important;\">{{ \"%.2f\" | format(ns.final_balance) }}</td>\r\n        <td style=\"width:30%; padding: 2px 0 !important;\"></td>\r\n        <td style=\"padding: 2px 0 !important;\"><b>Warehouse In Charge</b></td>\r\n        <td style=\"padding: 2px 0 !important;\">:</td>\r\n        <td style=\"padding: 2px 0 !important;\">{{doc.owner or ''}}</td>\r\n        <tr>\r\n            <td style=\"padding: 2px 0 !important;\"></td>\r\n            <td style=\"padding: 2px 0 !important;\"></td>\r\n            <td style=\"padding: 2px 0 !important;\"></td>\r\n            <td style=\"padding: 2px 0 !important;\"></td>\r\n            <td style=\"padding: 2px 0 !important;\"><b>Date</b></td>\r\n            <td style=\"padding: 2px 0 !important;\">:</td>\r\n            <td style=\"padding: 2px 0 !important;\">{{ frappe.utils.nowdate() }}</td>\r\n        </tr>\r\n    </tr>\r\n</table>",
